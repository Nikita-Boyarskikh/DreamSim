  },
  "scripts": {
    "start": "NODE_ENV=development webpack-dev-server --watch",
    "test": "NODE_ENV=testing jest --config=jest.config.js",
    "test:watch": "NODE_ENV=testing jest --config=jest.config.js --watch",
    "test:coverage": "NODE_ENV=testing jest --config=jest.config.js --coverage",
    "lint": "npm-run-all eslint jshint prettier stylelint",
    "lint:fix": "npm-run-all prettier:fix eslint:fix stylelint:fix",
    "eslint": "eslint --ext .json,.html,.js,.jsx --ignore-path=.gitignore src",
    "eslint:fix": "eslint --ext .json,.html,.js,.jsx --ignore-path=.gitignore --fix src",
    "jshint": "jshint --extract=auto -e .html,.js,.jsx src",
    "prettier": "prettier -c '**/*.@(js|jsx)'",
    "prettier:fix": "prettier --write '**/*.@(js|jsx)'",
    "stylelint": "stylelint '**/*.@(css|sass|scss|html)'",
    "stylelint:fix": "stylelint '**/*.@(css|sass|scss|html)' --fix",
    "build": "NODE_ENV=production webpack --env.API_URL=${API_URL}",
    "build:dev": "NODE_ENV=development webpack"
  },
  "os": [
    "darwin",
    "linux"
  ],
  "engines": {
    "npm": "6.8.0",
    "node": "11.12.0"
  },
